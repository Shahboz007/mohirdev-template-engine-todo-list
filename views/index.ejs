<%- include('header') -%>

<% if(allTasks.length<1) {%>
  <%- include('components/createNewTaskCard') -%>
<% } else{%>
  <%- include('components/todoItemAdd') -%>

  <% allTasks.forEach(task => {%>
    <%- include('components/todoItem', {
      id: task.id,
      title: task.title,text:task.desc, 
      status: task.status
      }) -%>  
  <%}) %>
<%}%>
<%- include('footer') -%>
